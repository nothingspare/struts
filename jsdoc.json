
{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs/gen",
    "template": "./node_modules/jaguarjs-jsdoc"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "jaguarjs-jsdoc",
    "theme": "jaguarjs-jsdoc",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}